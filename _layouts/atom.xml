---
---
<?xml version="1.0" encoding="utf-8"?>
<feed xmlns="http://www.w3.org/2005/Atom">
    <title>{{ page.feed_title }}</title>
    <link href="{{ page.feed_link }}"/>
    <id>{{ page.feed_id }}</id>
    <updated>{{ page.feed_updated }}</updated>
    <author>
        <name>{{ page.author_name }}</name>
        {% if page.author_email %}
        <email>{{ page.author_email }}</email>
        {% endif %}
    </author>
    {{ content }}
</feed> 